    "version": "0.2.0",
    "configurations": [


    {
        "name": "Lanzar Chrome contra el localhost",
        "request": "launch",
        "type": "pwa-chrome",
        "url": "http://localhost:8000/saludo",
        "webRoot": "${workspaceFolder}"
    },

    {
        "name": "Python: Django",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/manage.py",
        "console": "integratedTerminal",
        "args": [
            "runserver",
        ],
        "django": true
    }   
     
    ]
}
    